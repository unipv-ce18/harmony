root = true

[*]
end_of_line = lf
insert_final_newline = true
max_line_length = 120

[*.py]
indent_style = space
indent_size = 4

[*.{html,json,js,ts,tsx,css,scss}]
indent_style = space
indent_size = 2

[*.js]
ij_javascript_use_double_quotes = false

[*.{ts,tsx}]
ij_typescript_use_double_quotes = false
